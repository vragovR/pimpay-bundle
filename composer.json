{
  "name": "vragovr/pimpay-bundle",
  "description": "Pimpay API Bundle",
  "version": "0.0.1",
  "type": "library",
  "keywords": [
    "symfony",
    "bundle",
    "api",
    "pimpay"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Vragov",
      "email": "vragovR@yandex.ru"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-zlib": "*",
    "ext-dom": "*",
    "ext-soap": "*",
    "symfony/dependency-injection": "^2.8|^3.3",
    "symfony/config": "^2.8|^3.3",
    "symfony/http-kernel": "^2.8|^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "6.0.*"
  },
  "autoload": {
    "psr-4": {
      "PimpayBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PimpayBundle\\": "tests/"
    }
  }
}
